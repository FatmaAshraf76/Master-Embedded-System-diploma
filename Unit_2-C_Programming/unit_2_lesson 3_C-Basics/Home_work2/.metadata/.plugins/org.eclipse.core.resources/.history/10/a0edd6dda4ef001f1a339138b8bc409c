#include <stdio.h>
int main(void)
{
	float x,y,z;
	printf("Enter three numbers:");
	fflush(stdout);
	scanf("%f %f %f",&x,&y,&z);
	if(x>y && x>z)
	{
		printf("Largest number = %d",x);
	}
	else if(y>x && y>z)
	{
		printf("Largest number = %d",y);
	}
	else
	{
		printf("Largest number = %d",z);
	}
	return 0;
}
