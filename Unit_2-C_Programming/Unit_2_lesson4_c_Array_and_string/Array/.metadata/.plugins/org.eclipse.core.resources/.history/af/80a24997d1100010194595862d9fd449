#include <stdio.h>

int main(void) {
    int n, i, x, y;

    printf("Enter number of elements: ");
    fflush(stdout);
    scanf("%d", &n);

    int a[n], b[n + 1];

    printf("Enter %d elements:\n", n);
    fflush(stdout);
    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    return 0;
}
